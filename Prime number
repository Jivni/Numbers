class Numbers {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter Number");
        int n = sc.nextInt();
        boolean flag = false;
        for (int i = 2; i <= n / 2; i++) {
            if (n % i == 0) {
                flag = true;
                System.out.println("NOT PRIME");
                break;
            }
        }
        if (!flag) {
            System.out.println("PRIME");

        }
    }
}
